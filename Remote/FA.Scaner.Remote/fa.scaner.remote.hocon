akka {  
    stdout-loglevel = DEBUG
    loglevel = DEBUG
    log-config-on-start = on        
    actor {
		provider = remote

        debug {  
              receive = on 
              autoreceive = on
              lifecycle = on
              event-stream = on
              unhandled = on
        }

		
    }

	remote {
			dot-netty.tcp {
				port = 8091
				hostname = 127.0.0.1
			}
	}
}